{
  "name": "pnotify",
  "version": "5.0.0-dev",
  "description": "Beautiful dependency free notifications and prompts.",
  "keywords": [
    "notice",
    "notices",
    "notification",
    "notifications",
    "alert",
    "alerts",
    "web notifications",
    "prompts",
    "non blocking",
    "notify"
  ],
  "homepage": "https://github.com/sciactive/pnotify",
  "bugs": {
    "url": "https://github.com/sciactive/pnotify/issues"
  },
  "license": "Apache-2.0",
  "author": "Hunter Perrin",
  "files": [
    "dist",
    "src",
    "make.js"
  ],
  "directories": {
    "lib": "dist/umd"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sciactive/pnotify.git"
  },
  "scripts": {
    "build-dev": "ENV=development npm run build-umd && npm run build-cleanup",
    "watch": "ENV=development webpack --watch",
    "build": "npm run build-umd && npm run build-es && npm run build-cleanup",
    "build-umd": "webpack",
    "build-es": "node make.js",
    "build-cleanup": "node cleanup.js",
    "prepare-off": "npm run lint && npm run build",
    "lint": "eslint index.html make.js src/*.html src/*.js demo/*"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-classes": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-semistandard": "^15.0.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-standard": "^4.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "style-loader": "^1.1.3",
    "svelte-loader": "^2.13.6",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "svelte": "^3.18.1"
  }
}
